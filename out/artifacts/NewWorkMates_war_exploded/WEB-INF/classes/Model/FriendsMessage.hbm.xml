<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.FriendsMessage" table="Friends_Message" schema="dbo" catalog="WorkMates">
        <id name="id" column="id"/>
        <property name="sendId" column="sendId"/>
        <property name="recieverId" column="recieverId"/>
        <property name="message" column="message"/>
        <property name="Recieved" column="isRecieved"/>
        <property name="sendTime" column="sendTime"/>
    </class>
</hibernate-mapping>