<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Model.Project" table="Project" schema="dbo" catalog="WorkMates">
        <id name="id" column="id"/>
        <property name="masterId" column="masterId"/>
        <property name="createdTime" column="createdTime"/>
        <property name="totalHours" column="totalHours"/>
        <property name="Rewarded" column="isRewarded"/>
        <property name="rewardedCoin" column="rewardedCoin"/>
        <property name="title" column="title"/>
        <property name="description" column="description"/>
        <property name="projectAreaId" column="projectAreaId"/>
    </class>
</hibernate-mapping>